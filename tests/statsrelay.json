{
    "statsd":
    {
        "bind": "127.0.0.1:BIND_STATSD_PORT",
        "tcp_cork": true,
        "validate": true,
        "point_tag_regex": "\\.__([a-zA-Z][a-zA-Z0-9_]+)=[a-zA-Z0-9_/-]+",
        "shard_map":
        ["127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT",
         "127.0.0.1:SEND_STATSD_PORT"],
        "duplicate_to": [
            {
                "prefix": "test-1.",
                "suffix": ".suffix",
                "shard_map":["127.0.0.1:SEND_STATSD_PORT"]
            },
            {
                "prefix": "test-2.",
                "input_filter": "^(?=dontmatchme)",
                "shard_map":["127.0.0.1:SEND_STATSD_PORT"]
            }
        ]
    }
}
