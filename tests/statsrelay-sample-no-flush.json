{
    "statsd": {
        "bind": "127.0.0.1:BIND_STATSD_PORT",
        "duplicate_to": [
            {
                "prefix": "test-1.",
                "reservoir_size": 5,
                "sampling_threshold": 5,
                "sampling_window": 3,
                "shard_map": [
                    "127.0.0.1:SEND_STATSD_PORT"
                ],
                "suffix": ".suffix",
                "timer_flush_min_max": false,
                "timer_sampling_threshold": 5,
                "timer_sampling_window": 3
            }
        ],
        "point_tag_regex": "\\.__([a-zA-Z][a-zA-Z0-9_]+)=[a-zA-Z0-9_/-]+",
        "shard_map": [
            "127.0.0.1:9999"
        ],
        "validate": true
    }
}
