{
    "statsd": {
        "bind": "127.0.0.1:BIND_STATSD_PORT",
        "duplicate_to": [
            {
                "input_blacklist": "\\.pipeline\\.execute_command|(?<!git-server-)[0-9a-fA-F]{8}-(?:[0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}",
                "prefix": "test-production-iad.",
                "shard_map": [
                    "127.0.0.1:SEND_STATSD_PORT"
                ],
                "suffix": ".instance.test-production-iad-canary"
            }
        ],
        "point_tag_regex": "\\.__([a-zA-Z][a-zA-Z0-9_]+)=[a-zA-Z0-9_/-]+",
        "shard_map": [
            "127.0.0.1:9999"
        ],
        "validate": true
    }
}
